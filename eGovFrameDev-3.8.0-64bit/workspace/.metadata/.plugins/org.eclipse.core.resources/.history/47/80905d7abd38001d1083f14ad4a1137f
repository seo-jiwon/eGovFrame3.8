  <!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>사용자 관리페이지</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script type="text/javascript">
  function getSend(urladdr,param){
      var temp;
          $.ajax({
             type:'post',
             headers:{
                 "Content-Type":"application/json"
             },
             async :false,// ajax를 동기화(순서대로) 처리해야하는 경우 true로하거나 기술하지 않으면 비동기로 작동한다.
             url:urladdr,
             data:param,
             dataType:"json",
             success : function(result){
               temp= result;  
           },
         });
         return temp;
   }
  </script>
  </head>
  <body>
  <form>
  <input type="text" id="searchName">
  <input type="button" id="searchBtn" onclick='inputTxt()' value="검색">
  </form>
  <div>
  <span id="myspan">가나다</span>
  </div>
<p id="abc"></p>

 <script>
 
    function inputTxt(){
       var search = document.getElementById('searchName').value;
       console.log(search);
       var user = {};
       user["searchWord"] = search;
       
       var obj = new Object(); //컨트롤러의 리턴값(result)을 저장할 객체

       
      var jsonString = JSON.stringify(user);
         console.log(jsonString);

         var revdata = getSend('/ajaxProcess.do',jsonString);
         console.log(revdata);
         
         const json = JSON.stringify(revdata);
         document.getElementById("myspan").textContent=json;
    }

</script>
</body></html>